{
  "name": "starterkit",
  "version": "1.0.0",
  "author": "ischoe",
  "description": "JavaScript development environment starterkit",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check open:src lint:watch test:watch start-mockapi",
    "open:src": "babel-node buildScripts/srcServer.js",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel",
    "lint": "esw webpack.config.*, src, buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "tape -r babel-register src/**/*.test.js | faucet",
    "test:watch": "tape-watch -r babel-register src/**/*.test.js",
    "generate-mock-data": "babel-node buildScripts/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001",
    "clean-prod": "rm -rf ./prod && mkdir prod",
    "prebuild": "npm-run-all clean-prod test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/prodServer.js"
  },
  "dependencies": {
    "extract-text-webpack-plugin": "2.1.0",
    "axios": "^0.15.3"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-polyfill": "6.16.0",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "6.16.3",
    "chalk": "1.1.3",
    "compression": "1.6.2",
    "css-loader": "0.27.3",
    "enzyme": "^2.7.1",
    "eslint": "3.8.1",
    "eslint-plugin-import": "2.0.1",
    "eslint-watch": "2.1.14",
    "express": "4.14.0",
    "express-session": "^1.14.2",
    "faucet": "0.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "ignore-styles": "5.0.1",
    "jsdom": "9.8.0",
    "json-schema-faker": "0.3.6",
    "json-server": "0.8.22",
    "localtunnel": "1.8.1",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "open": "0.0.5",
    "react": "15.4.1",
    "react-addons-test-utils": "15.4.2",
    "react-dom": "15.4.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-modal": "1.6.5",
    "react-redux": "4.4.2",
    "react-router": "3.0.0",
    "react-router-redux": "4.0.2",
    "react-unit": "2.0.1",
    "redux": "3.4.0",
    "redux-logger": "2.7.4",
    "redux-thunk": "2.0.1",
    "rimraf": "2.5.4",
    "sinon": "^1.17.7",
    "style-loader": "0.13.1",
    "svg-url-loader": "^2.0.2",
    "tape": "4.6.2",
    "tape-watch": "2.2.4",
    "url-loader": "0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.5"
  }
}
